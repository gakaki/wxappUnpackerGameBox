button {
    padding-left: 0;
    padding-right: 0;
    margin-left: 0;
    margin-right: 0;
    background: transparent;
}

button:after {
    border: none;
}

button:active {
    background: transparent;
}

.weui-loading {
    margin: 0 5px;
    width: 20px;
    height: 20px;
    display: inline-block;
    vertical-align: middle;
    animation: weuiLoading 1s steps(12,end) infinite;
    background: transparent url(data:image/svg+xml;base64,) no-repeat;
    background-size: 100%;
}

.weui-loadmore {
    width: 65%;
    margin: 40rpx auto;
    line-height: 100rpx;
    font-size: 14px;
    text-align: center;
}

.weui-loadmore__tips {
    display: inline-block;
    vertical-align: middle;
}

.bottom-icon {
    margin-top: 30rpx;
    width: 100%;
    height: 150rpx;
    display: flex;
    justify-content: center;
}

.bottom-icon image {
    width: 206rpx;
    height: 50rpx;
    margin: 50rpx 0;
}

.network {
    margin: 30rpx 0;
    width: 100%;
    height: 404rpx;
}

.network image {
    width: 430rpx;
    height: 314rpx;
    float: left;
    margin-left: 160rpx;
}

.network .btn {
    width: 200rpx;
    margin-left: 275rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 6rpx;
    background: #6E7889;
    float: left;
    font-size: 30rpx;
    color: #fff;
}

.network .btn:active {
    opacity: 0.4;
}

.empty {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 1;
    display: flex;
    justify-content: center;
    align-items: center;
}

.empty .empty-img {
    width: 100%;
    height: 100%;
}

.hide {
    display: none!important;
}

@-webkit-keyframes ld-breath {
    0%,28%,48%,64%,76%,86%,93%,100% {
        animation-timing-function: ease-out;
    }

    14%,38%,56%,70%,81%,90%,97% {
        animation-timing-function: ease-in;
    }

    0% {
        transform: translateY(0%);
    }

    14% {
        transform: translateY(-27%);
    }

    28% {
        transform: translateY(0%);
    }

    38% {
        transform: translateY(-20%);
    }

    48% {
        transform: translateY(0%);
    }

    56% {
        transform: translateY(-16%);
    }

    64% {
        transform: translateY(0%);
    }

    70% {
        transform: translateY(-12%);
    }

    76% {
        transform: translateY(0%);
    }

    81% {
        transform: translateY(-7.5%);
    }

    86% {
        transform: translateY(0%);
    }

    90% {
        transform: translateY(-3%);
    }

    93% {
        transform: translateY(0%);
    }

    97% {
        transform: translateY(-1.5%);
    }

    100% {
        transform: translateY(0%);
    }
}

@keyframes ld-breath {
    0%,28%,48%,64%,76%,86%,93%,100% {
        animation-timing-function: ease-out;
    }

    14%,38%,56%,70%,81%,90%,97% {
        animation-timing-function: ease-in;
    }

    0% {
        transform: translateY(0%);
    }

    14% {
        transform: translateY(-27%);
    }

    28% {
        transform: translateY(0%);
    }

    38% {
        transform: translateY(-20%);
    }

    48% {
        transform: translateY(0%);
    }

    56% {
        transform: translateY(-16%);
    }

    64% {
        transform: translateY(0%);
    }

    70% {
        transform: translateY(-12%);
    }

    76% {
        transform: translateY(0%);
    }

    81% {
        transform: translateY(-7.5%);
    }

    86% {
        transform: translateY(0%);
    }

    90% {
        transform: translateY(-3%);
    }

    93% {
        transform: translateY(0%);
    }

    97% {
        transform: translateY(-1.5%);
    }

    100% {
        transform: translateY(0%);
    }
}

.animate {
    transition: all .3s ease-in-out;
}

.animate2 {
    transition: all .3s ease-out;
}

.animate3 {
    animation: 1s ld-breath 2 ease-out;
}

@-webkit-keyframes popanimate {
    0% {
        transform: scale(0.2);
    }

    to {
        transform: scale(1);
    }
}

@keyframes popanimate {
    0% {
        transform: scale(0.2);
    }

    to {
        transform: scale(1);
    }
}

.pop-box {
    position: fixed;
    z-index: 22;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.5);
    display: flex;
    justify-content: center;
    align-items: center;
}

.pop-box .pop {
    position: relative;
    z-index: 0;
    width: calc(100% - 260rpx);
    padding: 30rpx;
    border-radius: 10rpx;
    background: #fff;
    box-shadow: 0 10rpx 50rpx 0 rgba(0,0,0,0.2);
    animation: 200ms popanimate ease-in-out;
}

.pop-box .pop .title {
    padding: 20rpx 0;
    width: 100%;
    float: left;
    text-align: center;
    font-size: 36rpx;
    font-weight: bold;
    color: #F82C54;
}

.pop-box .pop .h1 {
    padding: 20rpx 0;
    width: 100%;
    float: left;
    text-align: center;
    font-size: 30rpx;
}

.pop-box .pop .text {
    padding: 20rpx 0;
    width: 100%;
    float: left;
    text-align: center;
    font-size: 28rpx;
    color: #6E7889;
}

.pop-box .pop .img {
    width: 100%;
    height: 300rpx;
    border-radius: 10rpx;
}

.pop-box .pop .pop-btn {
    height: 80rpx;
    line-height: 80rpx;
    float: left;
    width: 50%;
    margin-left: 25%;
    background: #F82C54;
    border-radius: 10rpx;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    margin-top: 20rpx;
}

.pop-box .pop .pop-btn:active {
    opacity: 0.5;
}

.pop-box .pop .close-btn,.pop-box .pop .solid-btn {
    display: flex;
    justify-content: center;
    position: absolute;
    z-index: 0;
    bottom: -130rpx;
    color: #fff;
}

.pop-box .pop .close-btn.rgicon-cl,.pop-box .pop .solid-btn.rgicon-cl {
    height: 100rpx;
    line-height: 100rpx;
    border: 3rpx solid #fff;
    text-align: center;
    width: 100rpx;
    height: 100rpx;
    border-radius: 100rpx;
    left: calc(50% - 50rpx);
    width: 100rpx;
}

.pop-box .pop .close-btn.solid-btn,.pop-box .pop .solid-btn.solid-btn {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    background: #F82C54;
    left: 0;
    font-size: 30rpx;
    border-radius: 100rpx;
}

.pop-box .pop .close-btn:active,.pop-box .pop .solid-btn:active {
    opacity: 0.5;
}

.pop-box .pop.login {
    background: transparent;
    position: relative;
    width: 468rpx;
    height: 440rpx;
    box-shadow: 0 10rpx 50rpx 0 rgba(0,0,0,0);
}

.pop-box .pop.login image {
    position: absolute;
    z-index: 0;
    left: 0;
    top: 0;
    width: 528rpx;
}

.pop-box .pop.login .text {
    width: 100%;
    position: absolute;
    left: 0;
    top: 170rpx;
    z-index: 1;
}

.pop-box .pop.login .pop-btn {
    position: absolute;
    left: 20rpx;
    top: 260rpx;
    width: 488rpx!important;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 20rpx;
    margin: 0;
    z-index: 1;
    background: #DD0000!important;
}

.invite-box {
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 2222;
    background: #fff;
}

.invite-box .invite-btn {
    position: absolute;
    z-index: 1;
    height: 80rpx;
    line-height: 80rpx;
    width: 300rpx;
    background: #fff;
    text-align: center;
    color: #000;
    border-radius: 80rpx;
    bottom: 90rpx;
    font-size: 36rpx;
    left: calc(50% - 150rpx);
}

.invite-box .invite-img {
    width: 100%;
    height: 100%;
}